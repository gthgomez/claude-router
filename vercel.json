{
  "buildCommand": "cd prismatix-frontend && npm install && npm run build",
  "outputDirectory": "prismatix-frontend/dist",
  "devCommand": "cd prismatix-frontend && npm run dev",
  "installCommand": "cd prismatix-frontend && npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
